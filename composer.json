{
  "name": "nickcheek/laravel-postman-generator",
  "description": "Generate Postman collections from Laravel routes",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Nickcheek\\LaravelPostmanGenerator\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Nicholas Cheek",
      "email": "nick@nicholascheek.com"
    }
  ],
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/routing": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/console": "^8.0|^9.0|^10.0|^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nickcheek\\LaravelPostmanGenerator\\LaravelPostmanGeneratorServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}